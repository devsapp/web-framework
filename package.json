{
  "name": "web-framework",
  "main": "./dist/index.js",
  "version": "0.0.10",
  "dependencies": {
    "@alicloud/fc2": "^2.2.2",
    "@alicloud/pop-core": "^1.7.10",
    "@serverless-devs/core": "^0.0.65",
    "deep-equal": "^2.0.5",
    "fs-extra": "^9.1.0",
    "inquirer": "^8.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/fs-extra": "^9.0.8",
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.10",
    "@types/node": "14",
    "f2elint": "^0.4.4",
    "jest": "^26.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.14",
    "typescript": "^3.9.7",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.4.0"
  },
  "scripts": {
    "dev": "npx tsc -w -p tsconfig.json",
    "prebuild": "rimraf dist",
    "copy-pulumi-fc": "mkdir -p dist/lib/pulumi-fc && cp -rf src/lib/pulumi-fc/* dist/lib/pulumi-fc",
    "copy-pulumi-install": "mkdir -p dist/scripts && cp -rf scripts/* dist/scripts/",
    "build:ts": "npx tsc -p tsconfig.json && npm run copy-pulumi-fc && npm run copy-pulumi-install",
    "build:component": "npx webpack --mode=production",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "f2elint scan",
    "fix": "f2elint fix",
    "typecheck": "npx tsc -p tsconfig.json --noEmit"
  },
  "husky": {
    "hooks": {
      "commit-msg": "f2elint exec commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "f2elint exec lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts}": "f2elint exec eslint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
